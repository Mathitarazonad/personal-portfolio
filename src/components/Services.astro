    box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;
  }

  .service-logo {
    display: flex;
    justify-content: center;
  }
  
  .service-info {
    text-align: center;
  }

  h3 {
    color: var(--dark-purple);
  }

  p {
    color: var(--dark-card-text);
    font-weight: 600;
  }

  svg {
    color: var(--dark-purple);
    width: 55px;
  }
</style>
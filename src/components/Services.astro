  }

  h2 {
    color: var(--light-purple);
    font-size: 1.75rem;
  }

  .services-container {
    display: flex;
    flex-direction: column;
    gap: 15px;
  }

  .service {
    min-height: 145px;
    padding: 15px;
    display: flex;
    gap: 10px;
    align-items: center;
